{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "jest": true
    },
    "rules": {
        "arrow-parens": ["error", "as-needed", {
            "requireForBlockBody": false
        }],
        "jsx-a11y/anchor-is-valid": ["error", {
            "specialLink": ["to"]
        }],
        "react/jsx-filename-extension": "off",
        "import/no-unresolved": "off"
    }
}
